{
    "name": "hanna-instruments/shopware-deployment",
    "type": "project",
    "license": "MIT",
    "description": "Hanna Instruments Shopware 6.6.7.0 Docker Deployment Package",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "assets:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "require": {
        "php": ">=8.1",
        "shopware/administration": "6.6.10.1",
        "shopware/core": "6.6.10.1",
        "shopware/elasticsearch": "6.6.10.1",
        "shopware/storefront": "6.6.10.1",
        "shopware/conflicts": "~0.4.0"
    },
    "require-dev": {},
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}